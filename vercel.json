{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "GH_TOKEN": "${GH_TOKEN}",
    "GITHUB_TOKEN": "${GH_TOKEN}",
    "VITE_GITHUB_USERNAME": "${VITE_GITHUB_USERNAME}",
    "VITE_GITHUB_USEREMAIL": "${VITE_GITHUB_USEREMAIL}",
    "VITE_FIREBASE_API_KEY": "@firebase_api_key",
    "VITE_FIREBASE_AUTH_DOMAIN": "@firebase_auth_domain",
    "VITE_FIREBASE_PROJECT_ID": "@firebase_project_id",
    "VITE_FIREBASE_STORAGE_BUCKET": "@firebase_storage_bucket",
    "VITE_FIREBASE_MESSAGING_SENDER_ID": "@firebase_messaging_sender_id",
    "VITE_FIREBASE_APP_ID": "@firebase_app_id",
    "VITE_FIREBASE_DATABASE_URL": "@firebase_database_url",
    "VITE_FIREBASE_MEASUREMENT_ID": "@firebase_measurement_id",
    "VITE_PINECONE_API_KEY": "${VITE_PINECONE_API_KEY}",
    "VITE_PINECONE_ENVIRONMENT": "${VITE_PINECONE_ENVIRONMENT}",
    "GRAFANA_ADMIN_PASSWORD": "${GRAFANA_ADMIN_PASSWORD}",
    "NODE_ENV": "production",
    "VITE_MODE": "production"
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "development": true,
      "staging": true,
      "preview": true
    }
  },
  "github": {
    "enabled": true,
    "silent": true,
    "autoAlias": true
  }
}