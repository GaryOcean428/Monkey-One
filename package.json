{
  "name": "monkey-one",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --force",
    "build": "vite build --mode production",
    "preview": "npm run build && firebase hosting:channel:deploy preview --expires 7d",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "type-check": "tsc --noEmit",
    "docs": "typedoc --out docs src",
    "clean": "rimraf node_modules/.vite dist",
    "validate:config": "firebase apps:list && firebase deploy --only hosting --dry-run",
    "predeploy": "npm run lint && npm run test && npm run type-check && npm run validate:config",
    "deploy": "npm run predeploy && npm run build && firebase deploy --only hosting || (echo 'Deployment failed' && exit 1)",
    "deploy:ci": "npm run build && firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" || (echo 'CI deployment failed' && exit 1)",
    "preview:pr": "npm run build && firebase hosting:channel:deploy pr-$PR_NUMBER --expires 7d || (echo 'PR preview failed' && exit 1)",
    "preview:branch": "npm run build && firebase hosting:channel:deploy ${BRANCH_NAME:-$(git branch --show-current)} --expires 7d || (echo 'Branch preview failed' && exit 1)",
    "firebase:login": "firebase login",
    "firebase:init": "firebase init",
    "firebase:deploy": "firebase deploy"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@huggingface/inference": "^2.6.4",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@monaco-editor/react": "^4.6.0",
    "@octokit/rest": "^20.0.2",
    "@pinecone-database/pinecone": "^2.0.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tensorflow/tfjs-core": "^4.17.0",
    "@tensorflow/tfjs-layers": "^4.17.0",
    "async-mutex": "^0.5.0",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.0.0",
    "framer-motion": "^11.0.3",
    "helmet": "^7.1.0",
    "highlight.js": "^11.9.0",
    "immer": "^10.0.3",
    "lucide-react": "^0.330.0",
    "marked": "^11.1.1",
    "marked-highlight": "^2.1.0",
    "preact": "^10.19.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "babel-plugin-import": "^1.13.8",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "firebase-tools": "^13.29.1",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.18.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.3"
  }
}
