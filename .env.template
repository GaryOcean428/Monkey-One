# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Model API Keys
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key
VITE_GROQ_API_KEY=your_groq_api_key
VITE_HUGGINGFACE_TOKEN=your_huggingface_token

# Default Model Configuration
VITE_DEFAULT_MODEL=llama-3.3-70b
VITE_FALLBACK_MODEL=llama3-groq-8b

# Tool Configuration
VITE_ENABLE_TOOL_GENERATION=true
VITE_MAX_TOOL_GENERATION_ATTEMPTS=3
VITE_TOOL_GENERATION_TIMEOUT=30000

# Security Configuration
VITE_ENABLE_SECURE_MODE=true
VITE_MAX_TOOL_MEMORY_MB=512
VITE_MAX_CONCURRENT_TOOLS=5

# Monitoring Configuration
VITE_ENABLE_TELEMETRY=true
VITE_LOG_LEVEL=info

# Development Configuration
VITE_DEV_MODE=true
VITE_MOCK_RESPONSES=false
