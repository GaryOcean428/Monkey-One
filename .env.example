# Public URL - Required for proper asset loading
VITE_PUBLIC_URL=https://monkey-one-nine.vercel.app

# API Configuration
VITE_API_BASE_URL=http://localhost:3004

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
VITE_SUPABASE_JWT_SECRET=your_supabase_jwt_secret

# Database Configuration
POSTGRES_URL=your_postgres_url
POSTGRES_PRISMA_URL=your_postgres_prisma_url
POSTGRES_URL_NO_SSL=your_postgres_url_no_ssl
POSTGRES_URL_NON_POOLING=your_postgres_url_non_pooling
POSTGRES_USER=your_postgres_user
POSTGRES_HOST=your_postgres_host
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DATABASE=your_postgres_database

# Storage Configuration
STORAGE_SUPABASE_ANON_KEY=your_storage_supabase_anon_key
STORAGE_POSTGRES_HOST=your_storage_postgres_host
STORAGE_POSTGRES_PASSWORD=your_storage_postgres_password

# Pinecone Configuration (for vector storage)
VITE_PINECONE_API_KEY=your_pinecone_api_key
VITE_PINECONE_ENVIRONMENT=your_pinecone_environment
VITE_PINECONE_INDEX_NAME=your_pinecone_index_name

# AI API Keys
VITE_OPENAI_API_KEY=your_openai_api_key
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key
VITE_GROQ_API_KEY=your_groq_api_key
VITE_XAI_API_KEY=your_xai_api_key
VITE_HF_API_KEY=your_huggingface_api_key
VITE_PERPLEXITY_API_KEY=your_perplexity_api_key

# Chroma Configuration
VITE_CHROMA_CLOUD_TOKEN=your_chroma_cloud_token

# Ollama Configuration
VITE_OLLAMA_BASE_URL=http://localhost:11434

# Authentication Settings
VITE_AUTH_ENABLED=false

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_BRAIN_VISUALIZER=true

# AI Configuration
VITE_AI_STREAMING_ENABLED=true
VITE_AI_MAX_TOKENS=4096
VITE_AI_MODEL_TEMPERATURE=0.7
VITE_AI_TOP_P=0.9
VITE_AI_FREQUENCY_PENALTY=0
VITE_AI_PRESENCE_PENALTY=0
VITE_AI_RETRY_ATTEMPTS=3

# Demo Mode
VITE_DEMO_MODE=true

# OIDC Configuration
VITE_OIDC_ENABLED=true
VITE_OIDC_ISSUER_MODE=team
VERCEL_OIDC_TOKEN=@vercel-oidc-token

# Skew Protection Configuration
VITE_VERCEL_SKEW_PROTECTION_ENABLED=1
VITE_VERCEL_DEPLOYMENT_ID=local-dev

# Proxy Server Configuration (for local development)
PROXY_PORT=3004

# AWS OIDC Configuration (for secure backend access)
AWS_ROLE_ARN=arn:aws:iam::123456789012:role/VercelOIDCRole
AWS_REGION=us-east-1

# Vercel Access Token (for deployment)
VERCEL_ACCESS_TOKEN=your_vercel_access_token
